<!-- Loop over each object in the projects array -->
{{#each techBlogPosts as |techBlogPost| }}
<div class="row mb-4 project">
  <div class="col-md-5">
    <h2>
      <!-- Call helper function with triple brackets to render the emoji span as HTML -->
      {{{get_emoji}}}
      <a href="/project/{{techBlogPost.id}}">{{techBlogPost.title}}</a>
    </h2>
    <p>Posted by {{techBlogPost.user.name}} on {{format_date techBlogPost.date_created}}</p>
  </div>
  <div class="col-md-7">
    <p>
      {{techBlogPost.description}}
    </p>
  </div>
</div>
{{/each}}
